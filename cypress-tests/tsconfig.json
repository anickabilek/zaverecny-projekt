        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": [
            "cypress",
            "node"
        ],
        "sourceMap": true
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ]
}